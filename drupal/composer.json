{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "ext-redis": "*",
        "composer/installers": "^2.2",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "^2.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/adv_varnish": "^4.0",
        "drupal/allowed_formats": "^3.0",
        "drupal/amswap": "^3.2",
        "drupal/autologout": "^1.4",
        "drupal/behat_ui": "^5.0@alpha",
        "drupal/big_pipe_sessionless": "^2.2",
        "drupal/block_content_permissions": "^1.11",
        "drupal/block_permissions": "^1.3",
        "drupal/captcha": "^2.0",
        "drupal/coffee": "^1.4",
        "drupal/config_split": "^2.0",
        "drupal/content_language_access": "^2.0@beta",
        "drupal/core": "^10",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-project-message": "^10",
        "drupal/core-recommended": "^10",
        "drupal/decoupled_preview": "^1.0@beta",
        "drupal/decoupled_router": "^2.0",
        "drupal/devel": "^5.2",
        "drupal/devel_generate": "^5.2",
        "drupal/dotenv": "^1.2",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/environment_indicator": "^4.0",
        "drupal/field_group": "^3.6",
        "drupal/gin": "^3.0@RC",
        "drupal/gin_login": "^2.1",
        "drupal/git_deploy": "^2.6",
        "drupal/google_tag": "^2.0",
        "drupal/hook_event_dispatcher": "^4.0",
        "drupal/index_now": "^2.0",
        "drupal/jsonapi_boost": "^2.0",
        "drupal/jsonapi_extras": "^3.26@beta",
        "drupal/jsonapi_hypermedia": "^1.9",
        "drupal/jsonapi_image_styles": "^3.0",
        "drupal/jsonapi_include": "^1.7",
        "drupal/jsonapi_menu_items": "^1.2",
        "drupal/jsonapi_pathauto_breadcrumbs": "^1.0",
        "drupal/jsonapi_resources": "^1.0",
        "drupal/jsonapi_user_resources": "^1.0@beta",
        "drupal/jsonapi_views": "^1.1",
        "drupal/leaflet": "^10.2",
        "drupal/libraries": "^4.0",
        "drupal/linkit": "^7.0@alpha",
        "drupal/login_security": "^2.0",
        "drupal/media_library_form_element": "^2.0",
        "drupal/menu_admin_per_menu": "^1.5",
        "drupal/multiple_registration": "^3.3",
        "drupal/next": "^2.0@beta",
        "drupal/openai": "^1.0@beta",
        "drupal/openapi": "^2.1",
        "drupal/openapi_jsonapi": "^3.0",
        "drupal/openapi_ui": "^1.0@RC",
        "drupal/openapi_ui_swagger": "^1.0",
        "drupal/openid_connect": "^1.4",
        "drupal/opquast_checklist": "^2.0",
        "drupal/paragraphs": "^1.16",
        "drupal/pathauto": "^1.11",
        "drupal/perimeter": "^3.0",
        "drupal/photoswipe": "^5.0",
        "drupal/purge": "^3.5",
        "drupal/purge_purger_http": "^1.2",
        "drupal/redirect": "^1.9",
        "drupal/redirect_after_login": "^3.0",
        "drupal/redis": "^1.7",
        "drupal/rename_admin_paths": "^3",
        "drupal/restui": "^1.21",
        "drupal/search_api": "^1.31",
        "drupal/search_api_solr": "^4.3",
        "drupal/seckit": "^2.0",
        "drupal/security_review": "^3.0@RC",
        "drupal/seo_checklist": "^5.2",
        "drupal/simple_multistep": "^2.0",
        "drupal/simple_oauth": "^6.0@beta",
        "drupal/simple_sitemap": "^4.1",
        "drupal/simplify": "^2.1",
        "drupal/site_audit": "^4.0@RC",
        "drupal/social_media_links": "^2.9",
        "drupal/stop_admin": "^1.4",
        "drupal/svg_image_field": "^2.3",
        "drupal/taxonomy_access_fix": "^4.0",
        "drupal/tmgmt": "^1.15",
        "drupal/token": "^1.13",
        "drupal/typed_entity": "^4.0",
        "drupal/upgrade_rector": "^2.0@alpha",
        "drupal/upgrade_status": "^4.3",
        "drupal/user_registrationpassword": "^2.0",
        "drupal/users_jwt": "^2.1",
        "drupal/view_unpublished": "^1.2",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/warmer": "^2.0",
        "drupal/webform": "^6.2",
        "drupal/webform_rest": "^4.0",
        "drupal/webprofiler": "^10",
        "drush/drush": "^12",
        "mnsami/composer-custom-directory-installer": "^2.0",
        "symfony/http-client": "^6.4",
        "theodo-group/llphant": "^0.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "cweagans/composer-patches": true,
            "mnsami/composer-custom-directory-installer": true,
            "simplesamlphp/composer-module-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library", "swagger-api/swagger-ui"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "patches": {
            "drupal/webform_rest": {
                "Add API endpoint for retrieving autocomplete options": "https://www.drupal.org/files/issues/2022-10-27/3317762-2.patch",
                "Automated Drupal 11 compatibility fixes for webform_rest": "https://www.drupal.org/files/issues/2024-03-25/webform_rest.4.1.0.rector.patch"

            },
            "drupal/amswap": {
                "Automated Drupal 11 compatibility fixes for amswap": "https://www.drupal.org/files/issues/2024-03-16/amswap.3.2.1.rector.patch"
            },
            "drupal/jsonapi_user_resources": {
                "Automated Drupal 11 compatibility fixes for jsonapi_user_resources": "https://www.drupal.org/files/issues/2024-06-18/jsonapi_user_resources.1.x-dev.rector.patch"
            },
            "drupal/simplify":{
                "Automated Drupal 11 compatibility fixes for simplify": "https://www.drupal.org/files/issues/2024-03-25/simplify.2.1.0.rector.patch"
            },
            "drupal/redirect_after_login": {
                "Drupal 11 compatibility": "https://www.drupal.org/files/issues/2024-07-22/redirect_after_login-3462725.patch"
            },
            "drupal/composer_deploy": {
                "Automated Drupal 11 compatibility fixes for composer_deploy": "https://www.drupal.org/files/issues/2024-03-16/composer_deploy.8.x-1.7.rector.patch"
            },
            "drupal/hook_event_dispatcher": {
                "Automated Drupal 11 compatibility fixes for hook_event_dispatcher": "https://www.drupal.org/files/issues/2024-07-15/hook_event_dispatcher.4.1.x-dev.rector.patch"
            },
            "drupal/imageapi_optimize": {
                "utomated Drupal 11": "https://www.drupal.org/files/issues/2024-04-04/imageapi_optimize.4.x-dev.rector.patch"
            },
            "drupal/jsonapi_user_resources": {
                "Automated Drupal 11 compatibility fixes for jsonapi_user_resources": "https://www.drupal.org/files/issues/2024-06-18/jsonapi_user_resources.1.x-dev.rector.patch"
            }
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    },
    "require-dev": {
        "behat/behat": "^3.14",
        "behat/mink": "^1.9",
        "behat/mink-browserkit-driver": "^2.2",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.7",
        "drevops/behat-screenshot": "^1.5",
        "drupal/drupal-extension": "^5.0",
        "emuse/behat-html-formatter": "^2.0",
        "jakoch/phantomjs-installer": "^2.1",
        "phpmd/phpmd": "^2.15",
        "phpspec/prophecy": "^1.18",
        "phpspec/prophecy-phpunit": "^2.1",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.6",
        "symfony/phpunit-bridge": "^7.0"
    }
}
